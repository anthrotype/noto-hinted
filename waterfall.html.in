<html lang="en">

<head>
  <meta http-equiv="Content-Type"
        content="text/html; charset=utf-8">
  <meta http-equiv="Content-Style-Type"
        content="text/css">

  <meta name="description"
        content="Noto fonts hinted with ttfautohint">
  <meta name="keywords"
        content="FreeType ttfautohint noto fonts">
  <meta name="author"
        content="Werner Lemberg">

  <style type="text/css">
    @font-face {
      font-family: "@font-name@";
      src: url("@font-name@.ttf");
      text-rendering: optimizeLegibility; }	

    span.noto { font-family: "@font-name@"; }
    p { white-space: nowrap; }
  </style>

  <title>@font-name@</title>
</head>

<body>
  <h1>@font-name@</h1>

  <p style="font-size: 9px;" lang="@lang@">9px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 10px;" lang="@lang@">10px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 11px;" lang="@lang@">11px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 12px;" lang="@lang@">12px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 13px;" lang="@lang@">13px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 14px;" lang="@lang@">14px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 15px;" lang="@lang@">15px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 16px;" lang="@lang@">16px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 17px;" lang="@lang@">17px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 18px;" lang="@lang@">18px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 19px;" lang="@lang@">19px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 20px;" lang="@lang@">20px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 21px;" lang="@lang@">21px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 22px;" lang="@lang@">22px:
    <span class="noto">@text@</span></p>
  <p style="font-size: 23px;" lang="@lang@">23px:
